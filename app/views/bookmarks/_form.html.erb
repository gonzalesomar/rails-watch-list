<div class="container">
  <%= simple_form_for [list, bookmark] do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.submit "Create Bookmark", class: "btn btn-primary" %>
  <% end %>

  <hr>

  <p><%= link_to "Back to list", list_path(list), class: "btn btn-info"%> </p>
</div>
